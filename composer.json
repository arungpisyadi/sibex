{
    "name": "arungpisyadi/sibex",
    "description": "A more straight forward package to use SendinBlue API V3 in Laravel environment (Ex : Express Solution). The package will enable you to handle SendInBlue various functions through Laravel Facade, mostly related to building email list and managing marketing campaign.",
    "license": "GNU",
    "authors": [{
        "name": "Arung Isyadi",
        "email": "isyadiarung@gmail.com",
        "homepage": "https://github.com/arungpisyadi"
    }],
    "homepage": "https://github.com/arungpisyadi/sibex",
    "keywords": ["Laravel", "SiBex"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "sendinblue/api-v3-sdk": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ArungPIsyadi\\SiBex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArungPIsyadi\\SiBex\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArungPIsyadi\\SiBex\\SiBexServiceProvider"
            ],
            "aliases": {
                "SiBex": "ArungPIsyadi\\SiBex\\Facades\\SiBex"
            }
        }
    }
}